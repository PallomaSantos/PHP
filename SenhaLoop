<?php
do{
    echo "\n Digite a sua senha:\n ";
    $senha = readline ();
    echo "\n Confirme a sua senha:\n ";
    $senhaFinal = readline();
    
    if ($senha == $senhaFinal){
        echo " Acesso liberado! ";}
    else {
        echo "\n Senha incorreta! Tente novamente.";
    }}
while($senha != $senhaFinal);
    
?>
